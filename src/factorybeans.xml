<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--静态工厂方法-->
    <bean id="car1" class="com.spring.factoryBeans.StaticCarFactory" factory-method="getCar">
        <constructor-arg value="audi"></constructor-arg>

    </bean>

    <bean id="factorycar" class="com.spring.factoryBeans.InstanceCarFactory"></bean>
    <bean id="car2" factory-bean="factorycar" factory-method="getCar">
        <constructor-arg value="ford"></constructor-arg>
    </bean>
</beans>